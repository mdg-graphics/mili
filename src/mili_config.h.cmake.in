/*    Mili Library
 *    mili_config.in generated by cmake
 */

// Configure command 
#cmakedefine CONFIG_CMD "@CONFIG_CMD@"

// Define to dummy `main' function (if any) required to link to the Fortran libraries.
#cmakedefine FC_DUMMY_MAIN

// Define if F77 and FC dummy `main' functions are identical.
#cmakedefine FC_DUMMY_MAIN_EQ_F77

// Define to a macro mangling the given C identifier (in lower and upper
// case), which must not contain underscores, for linking with Fortran. 
#cmakedefine FC_FUNC

// As FC_FUNC, but for C identifiers containing underscores.
#cmakedefine FC_FUNC_

// Definer developer to report bugs to
#cmakedefine PACKAGE_BUGREPORT "@PACKAGE_BUGREPORT@"

// Define date
#cmakedefine PACKAGE_DATE "@PACKAGE_DATE@"

// Date and time of this version of Mili
#cmakedefine PACKAGE_DATETIME "@PACKAGE_DATETIME@"

// Define to the full name of this package.
#cmakedefine PACKAGE_NAME "@PACKAGE_NAME@"

// Define to the full name and version of this package.
#cmakedefine PACKAGE_STRING "@PACKAGE_STRING@"

// Define to the version of this package.
#cmakedefine PACKAGE_VERSION "@PACKAGE_VERSION@"

// System information
#cmakedefine SYSTEM_INFO_STRING "@SYSTEM_INFO_STRING@"

// Time and date of build
#cmakedefine BUILD_TIMESTAMP "@BUILD_TIMESTAMP@"

// User
#cmakedefine BUILD_DEVELOPER "@BUILD_DEVELOPER@"

// Hash of git commit
#cmakedefine MILI_GIT_COMMIT_SHA1 "@MILI_GIT_COMMIT_SHA1@"

// Define Make, Compile and Link Commands
#cmakedefine MAKE_CMD "@MAKE_CMD@"
#cmakedefine COMPILE_CMD "@COMPILE_CMD@"
#cmakedefine LINK_CMD "@LINK_CMD@"
